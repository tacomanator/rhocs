    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "husky": "^0.13.2",
    "lint-staged": "^3.4.0",
    "onchange": "^3.2.1",
    "prettier": "^1.2.2"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "dependencies": {
    "ramda": "^0.23.0"
  }
